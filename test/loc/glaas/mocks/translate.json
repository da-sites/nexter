  "dnt-doc-rules": {
    "total": 20,
    "limit": 20,
    "offset": 0,
    "data": [
      { "block_scope": "modal-metadata, table-metadata, carousel, block-group-end,block-group-start, gnav-brand", "column": "*", "row": "*", "pattern": "*", "action": "dnt" },
      { "block_scope": "section-metadata, card-metadata, metadata, review, marketo, dc-converter-widget", "column": "1", "row": "*", "pattern": "*", "action": "dnt" },
      { "block_scope": "review", "column": "1", "row": "*", "pattern": "equals(Hide title || Tooltip delay || Initial Value)", "action": "dnt-row" },
      {
        "block_scope": "section-metadata",
        "column": "1",
        "row": "*",
        "pattern": "equals(style || background || tab || masonry || carousel || loader-color || loader-duration || product-name || dismissal-animation-duration || Dismissal-animation-count || dismissal-tooltip-count)",
        "action": "dnt-row"
      },
      {
        "block_scope": "metadata",
        "column": "1",
        "row": "*",
        "pattern": "equals(robots || serp-content-type || pageCreatedAt || translated || publishDate || productJcrID || primaryProductName || image || footer-promo-tag || jarvis-chat || Jarvis-chat || footer || header || universal-nav || gnav-source || app-prompt-entitlement || app-prompt-path || canonical || target || Target || personalization)",
        "action": "dnt-row"
      },
      { "block_scope": "metadata", "column": "1", "row": "*", "pattern": "except(title || description)", "action": "dnt-row" },
      { "block_scope": "card-metadata", "column": "1", "row": "*", "pattern": "equals(cardDate|| CardImage || primaryTag || Tags || ContentType)", "action": "dnt-row" },
      {
        "block_scope": "aside, media, marquee,marquee-anchors, text, z-pattern, brick, interactive-marquee",
        "column": "1",
        "row": "1",
        "pattern": "beginsWith(# || rgb || hsl || linear-gradient)",
        "action": "dnt"
      },
      { "block_scope": "marquee", "column": "*", "row": "1", "pattern": "beginsWith(# || rgb || hsl || linear-gradient)", "action": "dnt" },
      { "block_scope": "tabs", "column": "1", "row": "2", "pattern": "equals(active tab)", "action": "dnt-row" },
      { "block_scope": "tabs", "column": "1", "row": "3", "pattern": "equals(id)", "action": "dnt-row" },
      { "block_scope": "action-scroller", "column": "1", "row": "*", "pattern": "equals(Item width || item width || Item Width)", "action": "dnt" },
      { "block_scope": "martech-metadata", "column": "2", "row": "*", "pattern": "*", "action": "dnt" },
      { "block_scope": "quiz-marquee", "column": "1", "row": "*", "pattern": "equals(nested-fragments)", "action": "dnt-row" },
      { "block_scope": "quiz-marquee", "column": "1", "row": "*", "pattern": "equals(--- black)", "action": "dnt" },
      { "block_scope": "brick, homepage-brick", "column": "*", "row": "-2", "pattern": "*", "action": "dnt-row" },
      { "block_scope": "marquee", "column": "1", "row": "1", "pattern": "has(picture)", "action": "dnt-row" },
      { "block_scope": "hero-marquee", "column": "1", "row": "*", "pattern": "beginsWith(con-block-row)", "action": "dnt" },
      { "block_scope": "hero-marquee", "column": "1", "row": "1", "pattern": "beginsWith(# || rgb || hsl || linear-gradient)", "action": "dnt" },
      { "block_scope": "merch-card", "column": "1", "row": "3", "pattern": "*", "action": "dnt-row" }
